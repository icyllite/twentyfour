<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_media_item_bottom_sheet_dialog"
    app:startDestination="@id/mediaItemBottomSheetDialogFragment">

    <dialog
        android:id="@+id/mediaItemBottomSheetDialogFragment"
        android:name="git.icyllite.twentyfour.fragments.MediaItemBottomSheetDialogFragment"
        tools:layout="@layout/fragment_media_item_bottom_sheet_dialog">

        <action
            android:id="@+id/action_mediaItemBottomSheetDialogFragment_to_fragment_add_or_remove_from_playlists"
            app:destination="@+id/fragment_add_or_remove_from_playlists"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />

        <action
            android:id="@+id/action_mediaItemBottomSheetDialogFragment_to_fragment_album"
            app:destination="@+id/fragment_album"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />

        <action
            android:id="@+id/action_mediaItemBottomSheetDialogFragment_to_fragment_artist"
            app:destination="@+id/fragment_artist"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />

        <action
            android:id="@+id/action_mediaItemBottomSheetDialogFragment_to_fragment_genre"
            app:destination="@+id/fragment_genre"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />

    </dialog>

</navigation>
